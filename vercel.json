{
  "version": 1,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/server.js"
    }
  ],
  "env": {
    "APP_URL": "@APP_URL",
    "PORT": "@PORT",
    "MONGO_URI": "@MONGO_URI",
    "JWT_SECRET": "@JWT_SECRET",
    "JWT_REFRESH_SECRET": "@JWT_REFRESH_SECRET",
    "ALLOWED_ORIGINS": "@ALLOWED_ORIGINS",
    "LOG_LEVEL": "@LOG_LEVEL",
    "SESSION_SECRET": "@SESSION_SECRET",
    "GOOGLE_CLIENT_ID": "@GOOGLE_CLIENT_ID",
    "GOOGLE_CLIENT_SECRET": "@GOOGLE_CLIENT_SECRET",
    "FACEBOOK_APP_ID": "@FACEBOOK_APP_ID",
    "FACEBOOK_APP_SECRET": "@FACEBOOK_APP_SECRET",
    "EMAIL_USER": "@EMAIL_USER",
    "EMAIL_PASS": "@EMAIL_PASS",
    "PLAN_FREE_PRICE": "@PLAN_FREE_PRICE",
    "PLAN_FREE_CLICKS_LIMIT": "@PLAN_FREE_CLICKS_LIMIT",
    "PLAN_STARTER_PRICE": "@PLAN_STARTER_PRICE",
    "PLAN_STARTER_CLICKS_LIMIT": "@PLAN_STARTER_CLICKS_LIMIT",
    "PLAN_GROW_PRICE": "@PLAN_GROW_PRICE",
    "PLAN_GROW_CLICKS_LIMIT": "@PLAN_GROW_CLICKS_LIMIT",
    "PLAN_SCALE_PRICE": "@PLAN_SCALE_PRICE",
    "PLAN_SCALE_CLICKS_LIMIT": "@PLAN_SCALE_CLICKS_LIMIT"
  }
}
